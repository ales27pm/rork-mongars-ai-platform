{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "credentialsSource": "remote",
        "simulator": true
      },
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "credentialsSource": "remote",
        "simulator": false
      },
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "simulator": false,
        "credentialsSource": "remote",
        "autoIncrement": true
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6757726515",
        "bundleIdentifier": "app.27pm.monGARS"
      }
    }
  }
}
