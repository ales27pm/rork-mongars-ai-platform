{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "distribution": "store",
      "ios": {
        "simulator": false,
        "credentialsSource": "local"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "REPLACE_WITH_ASC_APP_ID",
        "ascApiKeyPath": "credentials/ios/appstoreconnect.p8",
        "ascApiKeyIssuerId": "REPLACE_WITH_ASC_API_KEY_ISSUER_ID",
        "ascApiKeyId": "REPLACE_WITH_ASC_API_KEY_ID"
      }
    }
  }
}
